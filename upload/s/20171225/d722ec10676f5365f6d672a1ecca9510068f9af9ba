    <script>
        function Count(a, h, tag, id) {
            //update count
            var jsonpara2 = "/Home/UpdateCountAdv";
            $.ajax({
                url: jsonpara2,
                type: "POST",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: "{ 'id' : " + id + " }",
                success: function (data) {
                    if (data.Data == true) {
                        if (tag == "_blank") {
                            window.open(h, "_newtab");
                        }
                        if (tag == "_parent") {
                            window.location.href = h;
                        }
                    }
                }
            });

        }
    </script>

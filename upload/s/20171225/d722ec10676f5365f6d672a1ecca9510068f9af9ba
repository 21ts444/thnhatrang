<script type="text/javascript">
    $(document).on('click', "#btnsearch", function () {
        sum();
    });
    $(document).on('change', "#searchbox", function () {
        sum();
    });
    $(document).on('keydown', "#searchbox", function () {
        if (e.which == 13) {
            sum();
        }
    });
    function sum() {
        var search_keyword_value = $("#searchbox").val();
        if (search_keyword_value != '') {
            $.ajax({
                url: "/Home/Search",
                type: "POST",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: JSON.stringify({ 'key': search_keyword_value }),
                success: function (data) {
                    if (data.Data != "") {
                        $('#search_comple').html(data.Data);
                        $('#search_comple').show();
                    }
                    else {
                        $('#search_comple').html("")
                        $('#search_comple').hide();
                    }
                }
            });
        }
    }
    $(document).live("click", function (e) {
        $('#search_comple').html("")
        $('#search_comple').hide();
    });

</script>

